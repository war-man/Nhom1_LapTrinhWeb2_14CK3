<script>
    $(document).ready(function () {
        $('.btn-link').popover({
            title: "<h4  style='margin:0;' class='title'>Thông tin khách hàng</h4>",
            content:fetchData,
            html: true,
            placement: 'right'
        });
        function fetchData() {
            var fetch_data = '';
            var element = $(this);
            var id = element.attr("id");
            $.ajax({
                url: "@Url.Action("ChiTietKhachHang","AAcount")",
                method: "POST",
                async: false,
                data: { id: id },
                success: function (data) {
                    fetch_data = data;
                }
            });
            return fetch_data;
        }
    });
</script>
